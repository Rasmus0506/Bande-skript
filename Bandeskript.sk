
options:
	#HVAD DET KOSTER AT OPRETTE EN BANDE
	price: 0
	
	#MAIN FARVEN (TIL TING SOM SKAL FREMHÆVES)
	mainColor: &3
	
	#MINDRE VIGTIGE FARVE
	secondColor: &f


on load:
	#PREFIX
	set {bande.global.prefix} to "{@mainColor}Bande &8|{@secondColor}"

on chat:
	if {bande.%uuid of player%} is set:
		if {bande.owner.%{bande.%uuid of player%}%} is player:
			set chat format to "&8[%{bande.color.%{bande.%uuid of player%}%}%**%{bande.%uuid of player%}%&8] &8[%{bande.color.%{bande.%uuid of player%}%}%%{bande.level.%{bande.%uuid of player%}%}%&8] &8: %colored player's prefix% &8: &7%player% &8➢ &7%message%"
		else if {bande.rank.%uuid of player%} = "Assistant":
			set chat format to "&8[%{bande.color.%{bande.%uuid of player%}%}%*%{bande.%uuid of player%}%&8] &8[%{bande.color.%{bande.%uuid of player%}%}%%{bande.level.%{bande.%uuid of player%}%}%&8] &8: %colored player's prefix% &8: &7%player% &8➢ &7%message%"
		else:
			set chat format to "&8[%{bande.color.%{bande.%uuid of player%}%}%%{bande.%uuid of player%}%&8] &8[%{bande.color.%{bande.%uuid of player%}%}%%{bande.level.%{bande.%uuid of player%}%}%&8] &8: %colored player's prefix% &8: &7%player% &8➢ &7%message%"
	if {bande.%uuid of player%} is not set:
		set chat format to "%colored player's prefix% &8: &7%player% &8➢ &7%message%" 

on death of player:
	attacker is a player:
		{bande.%uuid of attacker%} is set:
			victim has permission "vagt.sk":
				add 1 to {bande.vagt.kills.%{bande.%uuid of attacker%}%}
			else:
				add 1 to {bande.fange.kills.%{bande.%uuid of attacker%}%}

on damage of player:
	attacker is a player:
		{bande.%uuid of attacker%} is set:
			{bande.%uuid of victim%} is set:
				{bande.%uuid of attacker%} = {bande.%uuid of victim%}:
					{bande.damage.%{bande.%uuid of attacker%}%} is set:
						set {_chance} to a random integer between 0 and 100
						if {_chance} is between 0 and {bande.damage.%{bande.%uuid of attacker%}%}:
							cancel event
						else:
							stop
					else:
						stop
		
		
function bandeMain(p: player):
	set {_uuid} to uuid of {_p}
	set {_bandemain} to chest inventory with 5 rows named "{@mainColor}&lBANDE &8➜ {@mainColor}&lMAIN"
	set slot 0, 2, 4, 6, 8, 36, 38, 40, 42 and 44 of {_bandemain} to blue stained glass pane named " " with lore " "
	set slot 1, 3, 5, 7, 37, 39, 41 and 43 of {_bandemain} to white stained glass pane named " " with lore " "
	if {bande.%{_uuid}%} is not set:
		set slot 22 of {_bandemain} to name tag named "&3&n&oOPRET EN BANE" with lore "&7Tryk her||&7for at||&7oprette din||&7helt egen||&7&n&obande&7!||&c&nKOSTER&8: &c&n{@price}"
	else:
		set slot 25 of {_bandemain} to skull of ("%{bande.owner.%{bande.%{_uuid}%}%}%" parsed as a offline player) named "&3&nMEDLEMMER" with lore "&7Klik her for at se||&7Medlemmerne af din bande||&7Og redigere deres ranks osv."
		set slot 19 of {_bandemain} to redstone repeater named "&3&nINFORMATION" with lore "&bEjer&8: &7%{bande.owner.%{bande.%{_uuid}%}%}%||&bLevel&8: &7%{bande.level.%{bande.%{_uuid}%}%}%||&bI Banken&8: &7%{bande.bal.%{bande.%{_uuid}%}%}%||&bFange kills&8: &7%{bande.fange.kills.%{bande.%{_uuid}%}%}%||&bVagt kills&8: &7%{bande.vagt.kills.%{bande.%{_uuid}%}%}%"
		set slot 21 of {_bandemain} to emerald named "&3&nPENGE" with lore "&fAlt der har med penge at gøre||&8[&2+&8] &fBank||&8[&2+&8] &fShop||&8[&2+&8] &fLevel"
		set slot 23 of {_bandemain} to anvil named "&3&nKommandoer:" with lore "&8[&2+&8] &fBande opret||&8[&2+&8] &fBande forlad||&8[&2+&8] &fBande inviter||&8[&2+&8] &fBande accepter||&8[&2+&8] &fBande bank||&8[&2+&8] &fBande level||&8[&2+&8] &fBande shop||&8[&2+&8] &fBande promote||&8[&2+&8] &fBande demote||&8[&2+&8] &fBande kick"
	open {_bandemain} to {_p}

function bandeOpret(p: player, bande: string):
	set {_uuid} to uuid of {_p}
	if {bande.%{_uuid}%} is not set:
		if {_p}'s balance >= {@price}:
			set {bande.%{_uuid}%} to {_bande}
			set {bande.bal.%{_bande}%} to 0
			set {bande.owner.%{_bande}%} to "%{_p}%"
			add {_p} to {bande.members.%{_bande}%::*}
			add {_bande} to {bande.list::*}
			set {bande.color.%{_bande}%} to "&b"
			set {bande.level.%{_bande}%} to 1
			set {bande.max.members.%{_bande}%} to 3
			set {bande.fange.kills.%{_bande}%} to 0
			set {bande.vagt.kills.%{_bande}%} to 0
			subtract {@price} from {_p}'s balance
			send "%{bande.global.prefix}% Oprettede banden {@mainColor}%{_bande}%" to {_p}
		else:
			send "%{bande.global.prefix}% Dette har du ikke råd til &8({@secondColor}Koster&8: {@secondColor}{@price}&8)" to {_p}
	else:
		send "%{bande.global.prefix}% Du er allerede med i en bande!" to {_p}
		
function forladBande(p: player):
	set {_uuid} to uuid of {_p}
	if {bande.%{_uuid}%} is set:
		if {bande.owner.%{bande.%{_uuid}%}%} != {_p}:
			remove {_p} from {bande.members.%{bande.%{_uuid}%}%::*}
			delete {bande.%{_uuid}%}
			delete {bande.rank.%{_uuid}%}
			send "%{bande.global.prefix}% Du forlod din bande" to {_p}
		else:
			delete {bande.max.members.%{bande.%{_uuid}%}%}
			delete {bande.bal.%{bande.%{_uuid}%}%}
			delete {bande.owner.%{bande.%{_uuid}%}%}
			remove {bande.%{_uuid}%} from {bande.list::*}
			delete {bande.color.%{bande.%{_uuid}%}%}
			delete {bande.level.%{bande.%{_uuid}%}%}
			loop {bande.members.%{bande.%{_uuid}%}%::*}:
				loop-value != {_p}:
					set {_member} to ("%loop-value%" parsed as a offline player)
					set {_uuidmember} to uuid of {_member}
					delete {bande.rank.%{_uuidmember}%}
					delete {bande.%{_uuidmember}%}
					send "%{bande.global.prefix}% Din bande er dsv. blevet slettet da ejeren forlod banden" to loop-value
			delete {bande.members.%{bande.%{_uuid}%}%::*}
			delete {bande.%{_uuid}%}
			send "%{bande.global.prefix}% Din bande er blevet slettet." to {_p}

function bandeInvite(p: player, target: player):
	set {_uuid} to uuid of {_p}
	set {_dif} to difference between {last.invited.%{_target}%.%{bande.%{_uuid}%}%} and now
	set {_s} to size of {bande.members.%{bande.%{_uuid}%}%::*}
	if {_s} < {bande.max.members.%{bande.%{_uuid}%}%}:
		if {_dif} <= 1 minute:
			send "%{bande.global.prefix}% Der skal gå 1 min. imellem invites!" to {_p}
		else:
			set {bande.invite.%{_target}%} to {bande.%{_uuid}%}
			set {last.invited.%{_target}%.%{bande.%{_uuid}%}%} to now
			send "%{bande.global.prefix}% Du er blevet inviteret til at joine banden {@mainColor}%{bande.%{_uuid}%}%{@secondColor} Tryk her for at acceptere &8[<tooltip:&7Klik for at acceptere%nl%&7Invitationen fra {@mainColor}%{bande.%{_uuid}%}%!><cmd:/bande accepter> &a&lAccepter <reset>&8]" to {_target}
			send "%{bande.global.prefix}% Du har inviteret {@mainColor}%{_target}%{@secondColor} til din bande" to {_p}
	else:
		send "%{bande.global.prefix}% Der er ikke mere plads i din bande, /bande shop" to {_p}

function bandeAccepter(p: player):
	set {_s} to size of {bande.members.%{bande.invite.%{_p}%}%::*}
	if {_s} < {bande.max.members.%{bande.invite.%{_p}%}%}:
		set {_uuid} to uuid of {_p}
		set {bande.%{_uuid}%} to {bande.invite.%{_p}%}
		send "%{bande.global.prefix}% {@mainColor}%{_p}%{@secondColor} Joinede din bande!" to {bande.members.%{bande.%{_uuid}%}%::*}
		add {_p} to {bande.members.%{bande.%{_uuid}%}%::*}
		send "%{bande.global.prefix}% Du joinede banden {@mainColor}%{bande.%{_uuid}%}%" to {_p}
		set {bande.rank.%{_uuid}%} to "Medlem" 
		delete {bande.invite.%{_p}%}
		delete {last.invited.%{_p}%.%{bande.%{_uuid}%}%}
	else:
		send "%{bande.global.prefix}% Der er dsv. ikke plads i banden" to {_p}

function bandeMedlemmer(p: player):
	set {_uuid} to uuid of {_p}
	if {bande.%{_uuid}%} is set:
		set {_bandemedlemmer} to chest inventory with 6 rows named "{@mainColor}&lBANDE &8➜ {@mainColor}&lMEDLEMMER"
		set {_slot} to 9
		loop {bande.members.%{bande.%{_uuid}%}%::*}:
			if {bande.owner.%{bande.%{_uuid}%}%} is not loop-value:
				if ("%loop-value%" parsed as a offline player) is online:
					set {_status} to "&aOnline"
				else:
					set {_status} to "&cOffline"
				set {_member} to ("%loop-value%" parsed as a offline player)
				set {_uuidmember} to uuid of {_member}
				set slot {_slot} of {_bandemedlemmer} to skull of ("%{_member}%" parsed as a offline player) named "%loop-value%" with lore "&fRank&8: &a%{bande.rank.%{_uuidmember}%}%||&fStatus&8: %{_status}%"
				add 1 to {_slot}
		if ("%{bande.owner.%{bande.%{_uuid}%}%}%" parsed as a offline player) is online:
			set {_status} to "&aOnline"
		else:
			set {_status} to "&cOffline"
		set slot 4 of {_bandemedlemmer} to skull of ("%{bande.owner.%{bande.%{_uuid}%}%}%" parsed as a offline player) named "%{bande.owner.%{bande.%{_uuid}%}%}%" with lore "&fRank&8: &cOwner||&fStatus&8: %{_status}%"
		open {_bandemedlemmer} to {_p}


function bandeManage(p: player, t: text):
	close {_p}'s inventory
	set {_uuid} to uuid of {_p}
	set {_member} to ("%{_t}%" parsed as a offline player)
	if {_member} != {bande.owner.%{bande.%{_uuid}%}%}:
		set {_um} to uuid of {_member}
		set {_managemenu} to chest inventory with 3 rows named "{@mainColor}&lBANDE &8➜ {@mainColor}&lMANAGE &8➜ {@mainColor}&l%{_member}%"
		set slot 0, 2, 4, 6, 8, 18, 20, 22, 24 and 26 of {_managemenu} to blue stained glass pane named " " with lore " "
		set slot 1, 3, 5, 7, 19, 21, 23 and 25 of {_managemenu} to white stained glass pane named " " with lore " "
		if {bande.rank.%{_um}%} = "Medlem":
			set slot 11 of {_managemenu} to prismarine shard named "&3&nPROMOTE" with lore "&bMedlem &8➜ &bAssistant"
			set slot 13 of {_managemenu} to shiny redstone named "&3&nDEMOTE" with lore "&bKan ikke demotes mere"
		else if {bande.rank.%{_um}%} = "Assistant":
			set slot 11 of {_managemenu} to prismarine shard named "&3&nPROMOTE" with lore "&bAssistant &8➜ &cOwner"
			set slot 13 of {_managemenu} to redstone named "&3&nDEMOTE" with lore "&bAssistant &8➜ &bMedlem"
		set slot 15 of {_managemenu} to TNT named "&3&nKICK" with lore "&7Kick spilleren &b%{_member}%"
		set {chosen.manager.%{bande.%{_uuid}%}%} to {_member}
		open {_managemenu} to {_p}

function bandeKick(p: player, t: player):
	close {_p}'s inventory
	set {_uuid} to uuid of {_p}
	set {_um} to uuid of {_t}
	remove {_t} from {bande.members.%{bande.%{_uuid}%}%::*}
	delete {bande.%{_um}%}
	delete {bande.rank.%{_um}%}
	send "%{bande.global.prefix}% Du er blevet kicket fra din bande!" to {_t}
	send "%{bande.global.prefix}% {@mainColor}%{_t}% {@secondColor}er blevet kicket fra din bande!" to {bande.members.%{bande.%{_uuid}%}%::*}

function bandePromote(p: player, t: player):
	set {_uuid} to uuid of {_p}
	set {_um} to uuid of {_t}
	if {bande.rank.%{_um}%} = "Medlem":
		set {bande.rank.%{_um}%} to "Assistant"
		send "%{bande.global.prefix}% Du er blevet forfremmet til {@mainColor}Assistant{@secondColor}!" to {_t}
		send "%{bande.global.prefix}% Du har forfremmet {@mainColor}%{_t}% {@secondColor}til {@mainColor}Assistant{@secondColor}!" to {_p}
		bandeManage({_p}, "%{_t}%")
	else if {bande.rank.%{_um}%} = "Assistant":
		set {bande.owner.%{bande.%{_uuid}%}%} to "%{_t}%"
		set {bande.rank.%{_uuid}%} to "Medlem"
		send "%{bande.global.prefix}% Du er blevet forfremmet til {@mainColor}Owner{@secondColor}!" to {_t}
		send "%{bande.global.prefix}% Du har forfremmet {@mainColor}%{_t}% {@secondColor}til {@mainColor}Owner{@secondColor}!" to {_p}
		bandeManage({_p}, "%{_t}%")
	else:
		broadcast "%{bande.global.prefix}% Error! Kontakt en adminstrator, eller {@mainColor}PepeSquad_##6558 {@secondColor}på discord!"

function bandeDemote(p: player, t: player):
	set {_uuid} to uuid of {_p}
	set {_um} to uuid of {_t}
	if {bande.rank.%{_um}%} = "Assistant":
		set {bande.rank.%{_um}%} to "Medlem"
		send "%{bande.global.prefix}% Du er blevet demotet fra {@mainColor}Assistant{@secondColor} til {@mainColor}Medlem!" to {_t}
		send "%{bande.global.prefix}% Du har demotet {@mainColor}%{_t}% {@secondColor}fra {@mainColor}Assistant{@secondColor} til {@mainColor}Medlem!" to {_p}
		bandeManage({_p}, "%{_t}%")

