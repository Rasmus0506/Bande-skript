on inventory click:
	set {_name} to name of event-inventory
	if {_name} = "{@mainColor}&lBANDE &8➜ {@mainColor}&lMAIN":
		cancel event
		if {bande.%uuid of player%} is not set:
			if clicked slot = 22:
				close player's inventory
				set {bande.opretter.%uuid of player%} to true
				send "%{bande.global.prefix}% Skriv hvad din bande skal hedde i chatten, skriv {@mainColor}!cancel{@secondColor} hvis du vil annullere"
		else:	
			if clicked slot = 25:
				bandeMedlemmer(player)
			else if clicked slot = 21:
				bandePenge(player)
	else if {_name} = "{@mainColor}&lBANDE &8➜ {@mainColor}&lMEDLEMMER":
		cancel event
		if clicked item = player head:
			if {bande.owner.%{bande.%uuid of player%}%} is player:
				set {_target} to (name of clicked item)
				bandeManage(player, {_target})
	else if {_name} contains "{@mainColor}&lBANDE &8➜ {@mainColor}&lMANAGE &8➜":
		cancel event
		if clicked slot = 15:
			if {chosen.manager.%{bande.%uuid of player%}%} is set:
				cancel event
				bandeKick(player, {chosen.manager.%{bande.%uuid of player%}%})
			else:
				send "error"
