on chat:
	if {bande.opretter.%uuid of player%} is set:
		cancel event
		if message = "!cancel":
			delete {bande.opretter.%uuid of player%}
			send "%{bande.global.prefix}% Annullere oprettelse af banden.."
		else:
			length of message >= 3:
				length of message <= 15:
					if {bande.list::*} is set:
						loop {bande.list::*}:
							if loop-value = message:
								set {_taken} to true
						if {_taken} is not set:
							delete {bande.opretter.%uuid of player%}
							bandeOpret(player, message)
						else:
							send "%{bande.global.prefix}% Dette navn er dsv. taget"
					else:
						delete {bande.opretter.%uuid of player%}
						bandeOpret(player, message)
				else:
					send "%{bande.global.prefix}% Dette navn er dsv. for langt"
			else:
				send "%{bande.global.prefix}% Dette navn er dsv. for kort"

command /bande [<text>] [<text>] [<text>]:
	aliases: /gang
	trigger:
		if arg-1 is not set:
			bandeMain(player)
		else if arg-1 = "opret" or "create":
			if arg-2 is not set:
				set {bande.opretter.%uuid of player%} to true
				send "%{bande.global.prefix}% Skriv hvad din bande skal hedde i chatten, skriv {@mainColor}!cancel{@secondColor} hvis du vil annullere"
			else:
				if length of arg-2 >= 3:
					if length of arg-2 <= 15:
						if {bande.list::*} is set:
							loop {bande.list::*}:
								if loop-value = arg-2:
									set {_taken} to true
							if {_taken} is not set:
								bandeOpret(player, arg-2)
							else:
								send "%{bande.global.prefix}% Dette navn er dsv. taget"
						else:
							bandeOpret(Player, arg-2)
					else:
						send "%{bande.global.prefix}% Dette navn er dsv. for langt"
				else:
					send "%{bande.global.prefix}% Dette navn er dsv. for kort"
		if arg-1 is not "opret" or "create":
			if {bande.%uuid of player%} is set:
				if arg-1 is "forlad" or "leave":
					if arg-2 is not set:
						send "%{bande.global.prefix}% Er du sikker på du vil forlade din bande? Hvis du er ejer sletter dette din bande, du kan ændre ejer først &8[<tooltip:&7Klik for at forlade din bande!><cmd:/bande forlad yes> &a&lJA <reset>&8]"
					else:
						if arg-2 is "yes":
							forladBande(player)
						else:
							send "%{bande.global.prefix}% Er du sikker på du vil forlade din bande? Hvis du er ejer sletter dette din bande, du kan ændre ejer først &8[<tooltip:&7Klik for at forlade din bande!><cmd:/bande forlad yes> &a&lJA <reset>&8]"
				else if arg-1 = "invite" or "inviter":
					if arg-2 parsed as a player is set:
						if {bande.%uuid of player%} is set:
							if {bande.owner.%{bande.%uuid of player%}%} is player:
								bandeInvite(player, arg-2 parsed as a player)
							else if {bande.rank.%uuid of player%} = "Assistant":
								bandeInvite(player, arg-2 parsed as a player)
							else:
								send "%{bande.global.prefix}% Kun ejeren kan invitere folk til banden"
						else:
							send "%{bande.global.prefix}% Du er ikke medlem af nogen bande.."
					else:
						send "%{bande.global.prefix}% Korrekt brug: /Bande inviter <spiller>"
				else if arg-1 = "accepter", "acc" or "join":
					if {bande.invite.%player%} is set:
						if {bande.%player%} is not set:
							bandeAccepter(player)
						else:
							send "%{bande.global.prefix}% Du er allerede med i en bande"
					else:
						send "%{bande.global.prefix}% Du er ikke inviteret til nogen bande"
				else if arg-1 = "kick", "promote", "demote", "forfrem" or "ban":
					if arg-2 parsed as a player is set:
						bandeManage(player, arg-2)
					else:
						bandeMedlemmer(player)
				else if arg-1 = "bank":
					bandeBank(player)
				else if arg-1 is "deposit" or "indsæt":
					if arg-2 parsed as a integer is not set:
						set {_deposit} to chest inventory with 3 rows named "{@mainColor}&lBANDE &8➜ {@mainColor}&lDEPOSIT"
						set slot 0, 2, 4, 6, 8, 18, 20, 22, 24 and 26 of {_deposit} to blue stained glass pane named " " with lore " "
						set slot 1, 3, 5, 7, 19, 21, 23 and 25 of {_deposit} to white stained glass pane named " " with lore " "
						set slot 10 of {_deposit} to paper named "&a$&750" with lore " "
						set slot 12 of {_deposit} to paper named "&a$&7100" with lore " "
						set slot 14 of {_deposit} to paper named "&a$&7250" with lore " "
						set slot 16 of {_deposit} to paper named "&a$&7500" with lore " "
						open {_deposit} to player
					else:
						if player's balance >= arg-2 parsed as a integer:
							add arg-2 parsed as a integer to {bande.bal.%{bande.%uuid of player%}%}
							subtract arg-2 parsed as a integer from player's balance
							send "%{bande.global.prefix}% Du indsatte &a$&7%arg-2% {@secondColor}i din bande-bank (din bande har nu &a$&7%{bande.bal.%{bande.%uuid of player%}%}%{@secondColor})"
						else:
							send "%{bande.global.prefix}% Dette har du ikke råd til!"
				else if arg-1 is "withdraw":
					if {bande.owner.%{bande.%uuid of player%}%} is player:
						if arg-2 parsed as a integer is not set:
							set {_withdraw} to chest inventory with 3 rows named "{@mainColor}&lBANDE &8➜ {@mainColor}&lWITHDRAW"
							set slot 0, 2, 4, 6, 8, 18, 20, 22, 24 and 26 of {_withdraw} to blue stained glass pane named " " with lore " "
							set slot 1, 3, 5, 7, 19, 21, 23 and 25 of {_withdraw} to white stained glass pane named " " with lore " "
							set slot 10 of {_withdraw} to paper named "&a$&750" with lore " "
							set slot 12 of {_withdraw} to paper named "&a$&7100" with lore " "
							set slot 14 of {_withdraw} to paper named "&a$&7250" with lore " "
							set slot 16 of {_withdraw} to paper named "&a$&7500" with lore " "
							open {_withdraw} to player
						else:
							if {bande.bal.%{bande.%uuid of player%}%} >= arg-2 parsed as a integer:
								add arg-2 parsed as a integer to player's balance
								subtract arg-2 parsed as a integer from {bande.bal.%{bande.%uuid of player%}%}
								send "%{bande.global.prefix}% Du tog &a$&7%arg-2% {@secondColor}ud af din bande-bank (din bande har nu &a$&7%{bande.bal.%{bande.%uuid of player%}%}%{@secondColor})"
							else:
								send "%{bande.global.prefix}% Dette har din bande ikke råd til!"
					else:
						send "%{bande.global.prefix}% Kun ejeren af banden har adgang til denne funktion!"
				else if arg-1 is "level", "rankup" or "levelup":
					if {bande.owner.%{bande.%uuid of player%}%} is player:
						bandeLevel(player)
					else:
						send "%{bande.global.prefix}% Kun ejeren af banden har adgang til denne funktion!"
				else if arg-1 is "shop":
					bandeShop(player)
				else if arg-1 is "information" or "info":
					if arg-2 is set:
						if {bande.list::*} contains arg-2:
							send "&8&m---&7&m---&f&m---&7&m---&8&m---"
							send "{@mainColor}Ejer&8: {@secondColor}%{bande.owner.%arg-2%}%"
							send "{@mainColor}Level&8: {@secondColor}%{bande.level.%arg-2%}%"
							send "{@mainColor}Penge i banken&8: {@secondColor}%{bande.bal.%arg-2%}%"
							send "{@mainColor}Fange kills&8: {@secondColor}%{bande.fange.kills.%arg-2%}%"
							send "{@mainColor}Vagt kills&8: {@secondColor}%{bande.vagt.kills.%arg-2%}%"
							send "&8&m---&7&m---&f&m---&7&m---&8&m---"
						else:
							send "%{bande.global.prefix}% Denne bande findes ikke."
					else:
						send "%{bande.global.prefix}% Skriv venligst en bande"
				else if arg-1 is "farve" or "color":
					bandeFarve(player)
					
			else:
				send "%{bande.global.prefix}% Du er ikke medlem af nogen bande!"