function bandeLevelUp(p: player, current: int, next: int, fk: int, vk: int, m: int, money: int, newm: int):
	set {_uuid} to uuid of {_p}
	if {bande.level.%{bande.%{_uuid}%}%} = {_current}:
		if {bande.vagt.kills.%{bande.%{_uuid}%}%} >= {_vk}:
			if {bande.fange.kills.%{bande.%{_uuid}%}%} >= {_fk}:
				set {_size} to size of {bande.members.%{bande.%{_uuid}%}%::*}
				if {_size} >= {_m}:
					if {bande.bal.%{bande.%{_uuid}%}%} >= {_money}:
						close {_p}'s inventory
						set {bande.level.%{bande.%{_uuid}%}%} to {_next}
						subtract {_money} from {bande.bal.%{bande.%{_uuid}%}%}
						set {bande.max.members.%{bande.%{_uuid}%}%} to {_newm}
						loop {bande.members.%{bande.%{_uuid}%}%::*}:
							send loop-value title "&2&lBANDE LEVELUP!" with subtitle "&a&l%{_current}% &8➜ &a&l%{_next}%" for 4 seconds
							create fake explosion at location of loop-value
					else:
						send "%{bande.global.prefix}% Dette har din bande ikke nok penge til!" to {_p}
				else:
					send "%{bande.global.prefix}% Din bande har ikke nok medlemmer til dette!" to {_p}
			else:
				send "%{bande.global.prefix}% Din bande har ikke nok fange kills til dette!" to {_p}
		else:
			send "%{bande.global.prefix}% Din bande har ikke nok vagt kills til dette!" to {_p}

function bandeShop(p: player):
	set {_uuid} to uuid of {_p}
	set {_bandeshop} to chest inventory with 5 rows named "{@mainColor}&lBANDE &8➜ {@mainColor}&lSHOP"
	if {bande.damage.%{bande.%{_uuid}%}%} is not set:
		set {bande.damage.%{bande.%{_uuid}%}%} to 0
	if {bande.damage.price.%{bande.%{_uuid}%}%} is not set:
		set {bande.damage.price.%{bande.%{_uuid}%}%} to 250
	set slot 0, 2, 4, 6, 8, 36, 38, 40, 42 and 44 of {_bandeshop} to blue stained glass pane named " " with lore " "
	set slot 1, 3, 5, 7, 37, 39, 41 and 43 of {_bandeshop} to white stained glass pane named " " with lore " "
	if {bande.level.%{bande.%{_uuid}%}%} >= 4:
		if {bande.damage.%{bande.%{_uuid}%}%} < 100:
			set slot 21 of {_bandeshop} to iron sword named "&c&lBANDE DAMAGE" with lore "&7||&8&m--&7&m--&f&m--&7&m--&8&m--||&7||&a%{bande.damage.%{bande.%{_uuid}%}%}%&8/&c100||&7||&8&m--&7&m--&f&m--&7&m--&8&m--||&7"
		else:
			set slot 21 of {_bandeshop} to shiny iron sword named "&2&lBANDE DAMAGE" with lore "&7||&8&m--&7&m--&f&m--&7&m--&8&m--||&7||&a%{bande.damage.%{bande.%{_uuid}%}%}%&8/&c100||&7Færdig udviklet||&7||&8&m--&7&m--&f&m--&7&m--&8&m--||&7"
	else:
		set slot 21 of {_bandeshop} to barrier named "&4&l&nBANDE DAMAGE" with lore "&7||&8&m--&7&m--&f&m--&7&m--&8&m--||&7||&cDenne funktion er låst, indtil din bande kommer i level 4!||&7||&8&m--&7&m--&f&m--&7&m--&8&m--||&7"
	set slot 23 of {_bandeshop} to light green dye named "&2&lBANDE FARVE" with lore "&7||&8&m--&7&m--&f&m--&7&m--&8&m--||&7||&7Klik her!||&7||&8&m--&7&m--&f&m--&7&m--&8&m--"
	open {_bandeshop} to {_p}

function buyBandeDamage(p: player):
	set {_uuid} to uuid of {_p}
	if {bande.owner.%{bande.%{_uuid}%}%} is {_p}:
		if {bande.damage.%{bande.%{_uuid}%}%} < 100:
			if {bande.bal.%{bande.%{_uuid}%}%} >= {bande.damage.price.%{bande.%{_uuid}%}%}:
				close {_p}'s inventory
				bandeShop({_p})
				add 1 to {bande.damage.%{bande.%{_uuid}%}%}
				subtract {bande.damage.price.%{bande.%{_uuid}%}%} from {bande.bal.%{bande.%{_uuid}%}%}
				set {_+} to a random integer between 1 and 100
				add {_+} to {bande.damage.price.%{bande.%{_uuid}%}%}
			else:
				send "%{bande.global.prefix}% Dette har din bande ikke råd til!" to {_p}
		else:
			stop
	else:
		if {bande.rank.%{_p}%} is "Assistant":
			if {bande.damage.%{bande.%{_uuid}%}%} < 100:
				if {bande.bal.%{bande.%{_uuid}%}%} >= {bande.damage.price.%{bande.%{_uuid}%}%}:
					close {_p}'s inventory
					bandeShop({_p})
					add 1 to {bande.damage.%{bande.%{_uuid}%}%}
					subtract {bande.damage.price.%{bande.%{_uuid}%}%} from {bande.bal.%{bande.%{_uuid}%}%}
					set {_+} to a random integer between 1 and 100
					add {_+} to {bande.damage.price.%{bande.%{_uuid}%}%}
				else:
					send "%{bande.global.prefix}% Dette har din bande ikke råd til!" to {_p}
			else:
				stop
		else:
			close {_p}'s inventory
			send "%{bande.global.prefix}% Dette kan du ikke, da du hverken er assistant eller ejer af banden." to {_p}
			

